<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.database.repository.TestRepository">
    <select id="findByUserId" resultType="com.example.database.domain.Data">
        select *
        from test
        where user_id = #{user_id};
    </select>
</mapper>